    }
    argument {
        name: "output";
        type: pb_complex;
        value: "saliency_sandbox::utils::_IntensityImage";
    }
    input {
        node: "Seperate Channels";
        port: 1;
    }
}

node {
    name: "Convert to Intensity B";
