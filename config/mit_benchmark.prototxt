        port: 0;
    }
}

#
# Itti & Koch
#

node {
    name: "Orientation 0°";
    type: SaliencyOrientation;
    property {
        name: "theta";
        type: pb_float;
        value: "0.0f";
    }
    input {
        node: "Convert to Heatmap";
        port: 0;
    }
}

node {
    name: "Orientation 45°";
    type: SaliencyOrientation;
    property {
        name: "theta";
        type: pb_float;
        value: "45.0f * (M_PI/180.0f)";
